        if place_empty(x,y + 2){
            y += 2;
        }else{
            if instance_place(x,y+2,oWater) || instance_place(x,y+2,oLazerHor){
                y += 2;
            }else{
                //move_outside_solid(90,1);
                y = yprevious;
            }
        }
    }else{
        y = yprevious;
    }
    x = round(x/1) * 1;
    y = round(y/1) * 1;
}else{
    y = yprevious;
}

/// Get unstuck if stuck
